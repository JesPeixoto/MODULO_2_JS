
//Desafio 01

const numero1 = 10
const numero2 = 6

const soma = numero1 + numero2
const subtração = numero1 - numero2
const multiplicaçao = numero1 * numero2
const divisao = numero1 / numero2

const resultado = [ soma, subtração, multiplicaçao, divisao]

console.log("Soma:", resultado[0])
console.log("Subtração:", resultado[1])
console.log("Multiplicação:", resultado[2])
console.log("Divisão:", resultado[3])



